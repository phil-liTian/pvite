#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/litian.phil/phil/vite/phil-vite/core/node_modules/.pnpm/premove@4.0.0/node_modules/premove/node_modules:/Users/litian.phil/phil/vite/phil-vite/core/node_modules/.pnpm/premove@4.0.0/node_modules:/Users/litian.phil/phil/vite/phil-vite/core/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/litian.phil/phil/vite/phil-vite/core/node_modules/.pnpm/premove@4.0.0/node_modules/premove/node_modules:/Users/litian.phil/phil/vite/phil-vite/core/node_modules/.pnpm/premove@4.0.0/node_modules:/Users/litian.phil/phil/vite/phil-vite/core/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../premove/bin.js" "$@"
else
  exec node  "$basedir/../premove/bin.js" "$@"
fi
